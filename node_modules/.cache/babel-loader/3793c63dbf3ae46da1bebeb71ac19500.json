AC,kBAAD,CAAD,CAAsBiB,GAAtB,EAFnC,EAGA,KAAKR,QAAL,CAAc,kBAAd,IAAoCT,CAAC,CAAC,mBAAD,CAAD,CAAuBiB,GAAvB,EAHpC;AAIAD,SAAG,CAACE,cAAJ;AACAtB,WAAI,CACCuB,IADL,CACUpB,MAAK,GAAI,WADnB,EACgC,KAAKU,QADrC,EAEKN,IAFL,CAEU,kBAAO;AAAA,eAAM,MAAI,CAACU,MAAL,GAAcR,QAAQ,CAAC,MAAD,CAA5B;AAAA,OAFjB;AAGAL,OAAC,CAAC,OAAD,CAAD,CAAWiB,GAAX,CAAe,EAAf;AACAjB,OAAC,CAAC,QAAD,CAAD,CAAYiB,GAAZ,CAAgB,EAAhB;AACF;AAZO;AAvBI,CAAf","names":["axios","require","default","apiUri","$","created","get","then","countries","response","categories","name","data","formData","price","MenuItemCountry","MenuItemCategory","result","methods","onSubmit","evt","val","preventDefault","post"],"sourceRoot":"","sources":["/Users/Anastasia/WebstormProjects/un/src/components/AddMenuItem.vue"],"sourcesContent":["<template>\n  <form name=\"addMenuItem\" id=\"addMenuItem\">\n    <div>\n      <label for=\"name\">Name: </label>\n      <input\n          type=\"text\"\n          id=\"name\"\n          name=\"name\"\n          required\n      />\n    </div>\n    <div>\n      <label for=\"price\">Price: </label>\n      <input\n          type=\"number\"\n          id=\"price\"\n          name=\"price\"\n          required\n      />\n    </div>\n    <div>\n      <label for=\"MenuItemCountry_input\">Country: </label>\n      <input\n          type=\"hidden\"\n          id=\"MenuItemCountry_input\"\n          name=\"MenuItemCountry\"\n      />\n      <select id=\"MenuItemCountry\">\n        <option v-for=\"country in countries\" :key=\"country[countryId]\"> {{country.name}} </option>\n      </select>\n    </div>\n    <div>\n      <label for=\"MenuItemCategory_input\">Category: </label>\n      <input\n          type=\"hidden\"\n          id=\"MenuItemCategory_input\"\n          name=\"MenuItemCategory\"\n      />\n      <select id=\"MenuItemCategory\">\n        <option v-for=\"category in categories\" :key=\"category[categoryId]\"> {{category.name}} </option>\n      </select>\n    </div>\n\n    <input type=\"submit\" v-on:click=\"onSubmit\" id=\"btn\" value=\"Send\">\n    <div id=\"result\">{{this.result}}</div>\n  </form>\n\n</template>\n\n<script>\n\nconst axios = require('axios').default;\nconst apiUri = 'http://localhost:3000/';\nvar $ = require( \"jquery\" );\n\nexport default {\n  created () {\n      axios\n          .get(apiUri + 'country')\n          .then(response => (this.countries = response[\"data\"]));\n      axios\n          .get(apiUri + 'categories')\n          .then(response => (this.categories = response[\"data\"]));\n  },\n  name: 'AddMenuItem',\n  data(){\n    return{\n      countries: null,\n      categories: null,\n      formData: {\n        name: '',\n        price: '',\n        MenuItemCountry: '',\n        MenuItemCategory: ''\n      },\n      result:'',\n    }\n  },\n  methods: {\n    onSubmit (evt) {\n      this.formData['name'] = $(\"#name\").val(),\n      this.formData['price'] = $(\"#price\").val(),\n      this.formData['MenuItemCountry'] = $(\"#MenuItemCountry\").val(),\n      this.formData['MenuItemCategory'] = $(\"#MenuItemCategory\").val()\n      evt.preventDefault()\n      axios\n          .post(apiUri + 'menuItems', this.formData)\n          .then(response => (this.result = response[\"data\"]));\n      $(\"#name\").val('');\n      $(\"#price\").val('');\n    }\n\n  }\n}\n</script>\n\n<style scoped>\n\n\n  #addMenuItem {\n    width: 100%;\n    dislay: flex;\n    flex-direction: column;\n  }\n\n  #addMenuItem div {\n    display: flex;\n    flex-direction: row;\n    margin-bottom: 10px;\n  }\n\n  #addMenuItem div label {\n    display:block;\n    width: 150px;\n    font-weight: 700;\n  }\n\n  #addMenuItem div input, #addMenuItem div select {\n    width: 350px;\n    font-weight: 400;\n  }\n</style>"]}]}